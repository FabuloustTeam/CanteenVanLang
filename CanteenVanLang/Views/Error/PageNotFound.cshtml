@{
    Layout = null;
}

<header>
    <script src="~/Content/css-404/script.js"></script>
    <link href="~/Content/css-404/style.css" rel="stylesheet" />
</header>


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>404 - Canteen Van Lang</title>
